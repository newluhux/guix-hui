From f23b4563eb315e4da91e167d4cd12843e079e626 Mon Sep 17 00:00:00 2001
From: Lu Hui <luhux76@gmail.com>
Date: Thu, 20 Jul 2023 23:57:56 +0800
Subject: [PATCH 2/2] arch/arm/configs/allwinner_f1c100s_defconfig: add config
 for allwinner f1c100s device

---
 arch/arm/configs/allwinner_f1c100s_defconfig | 52 ++++++++++++++++++++
 1 file changed, 52 insertions(+)
 create mode 100644 arch/arm/configs/allwinner_f1c100s_defconfig

diff --git a/arch/arm/configs/allwinner_f1c100s_defconfig b/arch/arm/configs/allwinner_f1c100s_defconfig
new file mode 100644
index 000000000000..804718ce62aa
--- /dev/null
+++ b/arch/arm/configs/allwinner_f1c100s_defconfig
@@ -0,0 +1,52 @@
+CONFIG_NO_HZ_IDLE=y
+CONFIG_HIGH_RES_TIMERS=y
+CONFIG_CGROUPS=y
+CONFIG_BLK_DEV_INITRD=y
+CONFIG_PERF_EVENTS=y
+CONFIG_ARCH_SUNXI=y
+CONFIG_ARCH_MULTI_V5=y
+# CONFIG_ARCH_MULTI_V6 is not set
+# CONFIG_ARCH_MULTI_V7 is not set
+CONFIG_MACH_SUNIV=y
+CONFIG_HIGHMEM=y
+CONFIG_ARM_APPENDED_DTB=y
+CONFIG_ARM_ATAG_DTB_COMPAT=y
+CONFIG_CPU_FREQ=y
+CONFIG_CPUFREQ_DT=y
+CONFIG_CMA=y
+CONFIG_DEVTMPFS=y
+CONFIG_DEVTMPFS_MOUNT=y
+# CONFIG_WLAN is not set
+CONFIG_SERIAL_8250=y
+CONFIG_SERIAL_8250_CONSOLE=y
+CONFIG_SERIAL_8250_NR_UARTS=8
+CONFIG_SERIAL_8250_RUNTIME_UARTS=8
+CONFIG_SERIAL_8250_DW=y
+CONFIG_SERIAL_OF_PLATFORM=y
+# CONFIG_HW_RANDOM is not set
+CONFIG_SPI=y
+CONFIG_SPI_SUN6I=y
+CONFIG_SPI_SPIDEV=y
+CONFIG_WATCHDOG=y
+CONFIG_SUNXI_WATCHDOG=y
+CONFIG_REGULATOR=y
+CONFIG_REGULATOR_FIXED_VOLTAGE=y
+CONFIG_REGULATOR_GPIO=y
+CONFIG_MMC=y
+CONFIG_MMC_SUNXI=y
+CONFIG_NEW_LEDS=y
+CONFIG_LEDS_CLASS=y
+CONFIG_LEDS_GPIO=y
+CONFIG_LEDS_TRIGGERS=y
+CONFIG_LEDS_TRIGGER_HEARTBEAT=y
+CONFIG_LEDS_TRIGGER_DEFAULT_ON=y
+CONFIG_DMA_CMA=y
+CONFIG_PRINTK_TIME=y
+CONFIG_DEBUG_FS=y
+CONFIG_DEBUG_KERNEL=y
+CONFIG_DEBUG_LL=y
+CONFIG_DEBUG_SUNXI_UART0=y
+CONFIG_DEBUG_UART_PHYS=0x01c25000
+CONFIG_DEBUG_UART_VIRT=0x01c25000
+CONFIG_DEBUG_UART_8250_SHIFT=2
+CONFIG_EARLY_PRINTK=y
